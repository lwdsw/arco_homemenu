            <div class="arco-manager-dropdownitem-box">
                <div class="arco-manager-dropdownitem" t-on-click="setDefault">
                    <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.SET_DEFAULT"/>
                    <div class="arco-manager-text">Set Default</div>
                </div>
                <div class="arco-divider"/>
                <div class="arco-manager-dropdownitem" t-on-click="addNewHome">
                    <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.NEW"/>
                    <div class="arco-manager-text">Add New Page</div>
                </div>
                <div class="arco-manager-dropdownitem" t-on-click="removeHome">
                    <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.DELETE"/>
                    <div class="arco-manager-text">Remove Page</div>
                </div>
                <div class="arco-divider"/>
                <div class="arco-manager-dropdownitem" t-on-click="addNewFolder">
                    <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.NEWFOLDER"/>
                    <div class="arco-manager-text">Add New Folder</div>
                </div>
            </div>
        </div>
    </t>

