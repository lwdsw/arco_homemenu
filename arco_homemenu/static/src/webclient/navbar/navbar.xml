<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="web.NavBar.AppsMenu" owl="1">
        <Dropdown hotkey="'h'" title="'Home Menu'" class="'o_navbar_apps_menu arco-o_navbar_apps_menu'">
            <t t-set-slot="toggler">
                <i class="oi oi-apps" />
            </t>

            <div class="arco-home">

                <div class="arco-home-header">
                    <div class="arco-buttons">
                        <div t-attf-class="arco-dot circle #{homeMenuState.currentHomeId == 0 ? 'active' : ''}" t-on-click="() => this.toggleCustomMenuLayout(0)">
                        </div>
                        <t t-foreach="homeMenuState.homeData" t-as="home" t-key="home.id">
                            <div t-attf-class="arco-dot circle #{homeMenuState.currentHomeId == home.id ? 'active' : ''}" t-on-click="() => this.toggleCustomMenuLayout(home.id)">
                            </div>
                        </t>
                    </div>

                    <div class="arco-manager">
                        <div class="arco-manager-item mb4" t-on-click="changeHomeMode">
                            <div class="arco-manager-text">
                                <span t-if="homeMenuState.mode == 'edit'" t-esc="'Edit Mode'"/>
                                <span t-else="" t-esc="'Readonly Mode'"/>
                            </div>
                            <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.EDIT"/>
                        </div>
                        <div class="arco-manager-search" t-on-click="openSearch">
                            <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG.SEARCH"/>
                            <div id="searchContainer">
                                <input type="text" id="searchInput" placeholder="Search ..." t-on-input="(ev) => this.onSearchInput(ev)"/>
                                <div id="searchResults" t-if="homeMenuState.searchResults.length > 0">
                                    <t t-foreach="homeMenuState.searchResults" t-as="result" t-key="result_index">
                                        <a class="search-result-item" t-att-href="getMenuItemHref(result)" t-on-click="(ev) => this.onMenuClick(ev, result)">
                                            <div class="search-result-icon">
                                                <t t-if="result.webIconData">
                                                    <img t-if="result.webIconData.startsWith('P')" class="arco-app-img" t-attf-src="data:image/svg+xml;base64,#{result.webIconData}"/>
                                                    <img t-else="" class="arco-app-img" t-attf-src="data:image/png;base64,#{result.webIconData}"/>
                                                </t>
                                                <t t-else="">
                                                    <t t-if="result.webIcon">
                                                        <img class="arco-app-img" t-attf-src="#{result.webIcon.replace(',', '/')}"/>
                                                    </t>
                                                    <t t-else="">
                                                        <img class="arco-app-img" t-attf-src="/base/static/description/icon.png"/>
                                                    </t>
                                                </t>
                                            </div>
                                            <div class="search-result-name">
                                                <div class="name" t-out="result.name"/>
                                                <div class="display_name" t-out="result.displayName"/>
                                            </div>
                                        </div>
                                    </t>
                                </div>
                            </div>
                        </div>
                        <div class="arco-manager-menus" t-if="homeMenuState.mode == 'edit'">
                            <t t-call="arco_homemenu.NavBar.AppsMenu.ManagerSVG"/>
                        </div>
                    </div>
                </div>

                <div class="arco-home-menus" t-ref="mainMenusLayoutRef" t-attf-class="arco-layout #{homeMenuState.currentHomeId != 0 ? 'arco-hide' : 'arco-show'}">
                    <DropdownItem t-foreach="apps" t-as="app" t-key="app.id" class="{ 'o_app': true, focus: menuService.getCurrentApp() === app }" href="getMenuItemHref(app)" dataset="{ menuXmlid: app.xmlid, section: app.id }" onSelected="() => this.onNavBarDropdownItemSelection(app)">
                        <div class="arco-app-box">
                            <div class="arco-app-icon">
                                <t t-if="app.webIconData">
                                    <img t-if="app.webIconData.startsWith('P')" class="arco-app-img" t-attf-src="data:image/svg+xml;base64,#{app.webIconData}"/>
                                    <img t-else="" class="arco-app-img" t-attf-src="data:image/png;base64,#{app.webIconData}"/>
                                </t>
                                <t t-else="">
                                    <t t-if="app.webIcon">
                                        <img class="arco-app-img" t-attf-src="#{app.webIcon.replace(',', '/')}"/>
                                    </t>
                                    <t t-else="">
                                        <img class="arco-app-img" t-attf-src="/base/static/description/icon.png"/>
                                    </t>
                                </t>
                            </div>
                            <div class="arco-app-name" t-att-title="app.name" t-esc="app.name"/>
                        </div>
                    </DropdownItem>
                </div>

                <div class="arco-home-layout" t-ref="gridsterLayoutRef" t-attf-class="arco-layout #{homeMenuState.currentHomeId != 0 ? 'arco-show' : 'arco-hide'}">
                    <ArcoGridStack homeId="homeMenuState.currentHomeId" homeData="homeMenuState.homeData" mode="homeMenuState.mode"/>
                </div>
            </div>
        </Dropdown>
    </t>

